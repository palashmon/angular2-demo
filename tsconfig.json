{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es5",
            "es2015",
            "es2017",
            "dom",
            "scripthost"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "noEmitOnError": true
    },
    "exclude": [
        "node_modules"
    ]
}